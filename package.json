{
  "name": "musd-equity-report",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "update-routes": "node ./src/router/generate.js",
    "watch-routes": "nodemon --ext vue,js --watch ./src/routes --watch ./src/router/generate.js src/router/generate.js",
    "build-response-documentation": "node ./bin/create-documentation-response-data.js"
  },
  "dependencies": {
    "@jetstech/eslint-config-jetstech": "0.0.5",
    "cookies": "^0.8.0",
    "core-js": "^3.6.5",
    "cz-conventional-changelog": "^3.2.0",
    "d3": "^5.16.0",
    "d3-tip": "^0.9.1",
    "dotenv": "^8.2.0",
    "eslint-plugin-html": "^6.0.2",
    "firebase": "^7.15.5",
    "fs": "0.0.1-security",
    "module": "^1.2.5",
    "path": "^0.12.7",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "register-service-worker": "^1.7.1",
    "swiper": "^5.4.5",
    "uuid": "^8.2.0",
    "vue": "^2.6.11",
    "vue-route-generator": "^0.5.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
    "@vue/cli-plugin-pwa": "^4.4.0",
    "@vue/cli-plugin-unit-mocha": "^4.4.0",
    "@vue/cli-service": "^4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "chromedriver": "83",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "faker": "^4.1.0",
    "geckodriver": "^1.19.1",
    "nodemon": "^2.0.4",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stringify-object": "^3.3.0",
    "vue-auto-routing": "^0.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
